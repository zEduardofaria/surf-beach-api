    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.42",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
